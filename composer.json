{
    "name": "symfox/symfox",
    "description": "The Symfox Framework.",
    "keywords": ["framework", "symfox"],
    "license": "MIT",
    "type": "project",
    "require": {
        "doctrine/cache": "^1.8",
        "falc/flysystem-local-symlink-plugin": "^1.2",
        "illuminate/database": "^5.0",
        "league/flysystem": "^1.0",
        "nesbot/carbon": "^2.34",
        "swiftmailer/swiftmailer": "^6.2",
        "symfony/cache": "^5.0",
        "symfony/dotenv": "^5.2",
        "vlucas/phpdotenv": "^4.1"
    },
    "autoload": {
        "classmap": [
           
        ],
        "psr-4": {
            "App\\" : "app", 
            "App\\Module\\" : "app/modules", 
            "App\\Service\\" : "app/services", 
            "Symfox\\" : "vendor/symfox",
            "Symfony\\" : "vendor/symfony"
        }, 
        "files": [
            "config/constants.php",
            "config/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            
        }
    },
    "extra": {
        
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
