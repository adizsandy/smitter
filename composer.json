{
    "name": "smitter/smitter",
    "description": "Agile, Extensible, Modular PHP Framework",
    "keywords": ["framework", "smitter"],
    "license": "MIT",
    "type": "project",
    "require": {  
        "smitter/framework": "1.1.0"  
    },
    "autoload": {
        "classmap": [
           
        ],
        "psr-4": {
            "App\\" : "app", 
            "Boot\\" : "boot",   
            "Library\\" : "lib"
        }, 
        "files": [ 
            "misc/helpers/constants.php",
            "misc/helpers/functions.php" 
        ]
    },
    "autoload-dev": {
        "psr-4": {
            
        }
    },
    "extra": {
        
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
