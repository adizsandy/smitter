{
    "name": "symfox/symfox",
    "description": "The Symfox Framework",
    "keywords": ["framework", "symfox"],
    "license": "MIT",
    "type": "project",
    "require": { 
        "symfox/framework": "^21.17"
    },
    "autoload": {
        "classmap": [
           
        ],
        "psr-4": {
            "App\\" : "app", 
            "Boot\\" : "boot",  
            "App\\Module\\" : "app/modules", 
            "App\\Service\\" : "app/services", 
            "Symfox\\" : "vendor/symfox/framework/src", 
            "Symfony\\" : "vendor/symfony"
        }, 
        "files": [ 
            "app/helpers/constants.php",
            "app/helpers/functions.php" 
        ]
    },
    "autoload-dev": {
        "psr-4": {
            
        }
    },
    "extra": {
        
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
